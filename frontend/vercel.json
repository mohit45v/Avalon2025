{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/admin", "destination": "/" },
    { "source": "/admin/login", "destination": "/" },
    { "source": "/admin/participants", "destination": "/" },
    { "source": "/admin/verified", "destination": "/" },
    { "source": "/admin/queries", "destination": "/" },
    { "source": "/admin/analytics", "destination": "/" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}